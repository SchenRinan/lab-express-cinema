<div id="movies-cont">
    <h1>Cinema Ironhack</h1>
    <h2>Click on the movie to the showtimes!</h2>
    <section>
        {{#each item}}
        <div class="sml-box">
            <img src="{{image}}" alt="{{title}}" srcset="">
            <h3>{{title}}</h3>
            <h4><a href="/movie/{{id}}">See more</a></h4>
        </div>
        {{/each}}
    </section>
</div>